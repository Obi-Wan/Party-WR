<root>
  <effects>
    <hover>true</hover> <!-- boolean -->
    <expand>true</expand> <!-- boolean -->
  </effects>
  <structure>
<![CDATA[
<body [REQUESTING-CODE]>
[LAYERS]
  <div class="mainBox">
    <img src="images/PartyHardApproved.png" class="approved" alt="" />
[BANNER-TOP]
    <div class="banner">
      <img src="[TEMPLATE]/logo.png" class="logo" alt="Logo" />
      <img src="[TEMPLATE]/titolo-summer.png" class="banner" alt="banner" />
      <img src="[TEMPLATE]/titolo-chiara.png" class="sub_banner" alt="banner" />
    </div>
    <div class="blocco_centrale">
[MENU]
      <div class="container_corpo">
        <div class="mid_container_corpo">
          <div class="topper-corpo topper-bottomer">
            <span class="corner-hl corner"> </span>
            <span class="corner-hr corner"> </span>
          </div>
          <div class="corpo">
[CONTENTS]
          </div>
          <div class="bottomer-corpo topper-bottomer">
            <span class="corner-ll corner"> </span>
            <span class="corner-lr corner"> </span>
          </div>
        </div>
      </div>
[BANNER-BOTTOM]
    </div>
  </div>
</body>
]]>
  </structure>
  <menu>
    <initCode>
<![CDATA[      <div class="container_menu"><div class="menu">]]>
    </initCode>
    <closeCode>
<![CDATA[      </div></div>]]>
    </closeCode>
    <menuButton>
      <code><![CDATA[
        <a href="[HREF]" class="button_menu_container">
          <img id="[NAME]" src="[IMAGE-NORMAL]" class="button_menu" alt="[NAME]" onmouseover="mouseOver('[NAME]')" onmouseout="mouseOut('[NAME]')" />
        </a>]]>
      </code>
    </menuButton>
    <submenuButton>
      <code><![CDATA[
          <a href="[HREF]" class="button_menu_container">
            <img id="[NAME]" src="[IMAGE-NORMAL]" class="button_submenu" alt="[LEAF]" onmouseover="mouseOver('[NAME]')" onmouseout="mouseOut('[NAME]')" />
          </a>]]>
      </code>
    </submenuButton>
    <expandableButton>
      <code><![CDATA[
        <div class="button_menu_container">
          <img id="[NAME]" src="[IMAGE-NORMAL]" class="button_menu" alt="[NAME]" onmouseover="mouseOver('[NAME]')" onmouseout="mouseOut('[NAME]')" onclick="fold_unfold('[NAME]')" />
        </div>]]>
      </code>
    </expandableButton>
  </menu>
  <loading>
    <code><![CDATA[
  <div id="loadingFrame">
    <img id="loading_image" src="[LOADING-IMAGE]">

  </div>]]>
    </code>
    <objects>
    </objects>
  </loading>
  <gallery>
    <code><![CDATA[
  <img id="darkLayer" class="dark_layer" src="[TEMPLATE]/dark_layer.png" />
  <div id="displayerFrame">
    <img id="buttons_bar" src="[BUTTONS_BAR-NORMAL]">
    <div id="buttons_container" [BUTTONS_BAR-FUNCTIONS]>
      <img id="first"/>
      <img id="previous"/>
      <img id="next"/>
      <img id="last"/>
    </div>
   <img id="close"/>
   <img id="photoObject" src="[TEMPLATE]/blank.png" />
  </div>]]>
    </code>
    <objects>
      <gallery-button>
        <name>close</name>
        <hover>
          <type>classic</type>
          <normalImg>button_close_normal.png</normalImg>
          <hoverImg>button_close_hover.png</hoverImg>
        </hover>
      </gallery-button>
      <gallery-button>
        <name>next</name>
        <hover>
          <type>classic</type>
          <normalImg>button_next_normal.png</normalImg>
          <hoverImg>button_next_hover.png</hoverImg>
        </hover>
      </gallery-button>
      <gallery-button>
        <name>previous</name>
        <hover>
          <type>classic</type>
          <normalImg>button_previous_normal.png</normalImg>
          <hoverImg>button_previous_hover.png</hoverImg>
        </hover>
      </gallery-button>
      <gallery-button>
        <name>first</name>
        <hover>
          <type>classic</type>
          <normalImg>button_first_normal.png</normalImg>
          <hoverImg>button_first_hover.png</hoverImg>
        </hover>
      </gallery-button>
      <gallery-button>
        <name>last</name>
        <hover>
          <type>classic</type>
          <normalImg>button_last_normal.png</normalImg>
          <hoverImg>button_last_hover.png</hoverImg>
        </hover>
      </gallery-button>
      <gallery-bar>
        <name>buttons</name>
        <hover>
          <type>classic</type>
          <normalImg>buttons_bar_normal.png</normalImg>
          <hoverImg>buttons_bar_hover.png</hoverImg>
        </hover>
      </gallery-bar>
    </objects>
  </gallery>
</root>

<root>
  <effects>
    <hover>true</hover> <!-- boolean -->
    <expand>true</expand> <!-- boolean -->
  </effects>
  <structure>
<![CDATA[
<body [REQUESTING-CODE]>
[LAYERS]
  <div class="mainBox">
    <img src="images/PartyHardApproved.png" class="approved" alt="" />
[BANNER-TOP]
    <div class="banner">
      <img src="[TEMPLATE]/logo.png" class="logo" alt="Logo" />
      <img src="[TEMPLATE]/titolo-summer.png" class="banner" alt="banner" />
      <img src="[TEMPLATE]/titolo-chiara.png" class="sub_banner" alt="banner" />
    </div>
    <div class="blocco_centrale">
[MENU]
      <div class="container_corpo">
        <div class="mid_container_corpo">
          <div class="topper-corpo topper-bottomer">
            <span class="corner-hl corner"> </span>
            <span class="corner-hr corner"> </span>
          </div>
          <div class="corpo">
[CONTENTS]
          </div>
          <div class="bottomer-corpo topper-bottomer">
            <span class="corner-ll corner"> </span>
            <span class="corner-lr corner"> </span>
          </div>
        </div>
      </div>
[BANNER-BOTTOM]
    </div>
  </div>
</body>
]]>
  </structure>
  <menu>
    <initCode>
<![CDATA[      <div class="container_menu"><div class="menu">]]>
    </initCode>
    <closeCode>
<![CDATA[      </div></div>]]>
    </closeCode>
    <menuButton>
      <code>
<![CDATA[        <a href="[HREF]" class="menu_button"><img id="[NAME]" src="[IMAGE-NORMAL]" class="button_menu" alt="[NAME]" onmouseover="mouseOver('[NAME]')" onmouseout="mouseOut('[NAME]')" /></a>]]>
      </code>
      <type>input</type>
    </menuButton>
    <submenuButton>
      <code>
<![CDATA[          <a href="[HREF]" class="menu_button"><img id="[NAME]" src="[IMAGE-NORMAL]" class="button_submenu" alt="[LEAF]" onmouseover="mouseOver('[NAME]')" onmouseout="mouseOut('[NAME]')" /></a>]]>
      </code>
      <type>input</type>
    </submenuButton>
    <expandableButton>
      <code>
<![CDATA[        <img id="[NAME]" src="[IMAGE-NORMAL]" class="button_menu" alt="[NAME]" onmouseover="mouseOver('[NAME]')" onmouseout="mouseOut('[NAME]')" onclick="fold_unfold('[NAME]')" />]]>
      </code>
      <type>img</type>
    </expandableButton>
  </menu>
  <loading>
    <code>
<![CDATA[
  <div id="loadingFrame" class="loading_frame">
    <img id="buttons_bar" class="loading_image" src="[LOADING-IMAGE]">

  </div>
]]>
    </code>
    <objects>
    </objects>
  </loading>
  <gallery>
    <code>
<![CDATA[
  <img id="darkLayer" class="dark_layer" src="[TEMPLATE]/dark_layer.png" />
  <img id="displayerFrameBackground" class="displayer_frame_background" src="[TEMPLATE]/sfondo_photo_frame.png" />
  <div id="displayerFrame" class="displayer_frame">
    <img id="buttons_bar" class="buttons_bar" src="[BUTTONS_BAR-NORMAL]" onclick="[BUTTONS_BAR-FUNCTION]">
    <div id="buttons_container" class="buttons_container" onmouseover="mouseOver('buttons_bar')" onmouseout="mouseOut('buttons_bar')">
      <img id="first" class="first_button" src="[FIRST-NORMAL]" onmouseover="mouseOver('first')" onmouseout="mouseOut('first')" />
      <img id="previous" class="previous_button" src="[PREVIOUS-NORMAL]" onmouseover="mouseOver('previous')" onmouseout="mouseOut('previous')" />
      <img id="next" class="next_button" src="[NEXT-NORMAL]" onmouseover="mouseOver('next')" onmouseout="mouseOut('next')" />
      <img id="last" class="last_button" src="[LAST-NORMAL]" onmouseover="mouseOver('last')" onmouseout="mouseOut('last')" />
    </div>
   <img id="close" class="close_button" src="[CLOSE-NORMAL]" onmouseover="mouseOver('close')" onmouseout="mouseOut('close')" />
      <img id="photoObject" class="photo_object" src="[TEMPLATE]/blank.png" />
  </div>
]]>
    </code>
    <objects>
      <gallery-button>close</gallery-button>
      <gallery-button>next</gallery-button>
      <gallery-button>previous</gallery-button>
      <gallery-button>first</gallery-button>
      <gallery-button>last</gallery-button>
      <gallery-bar>buttons</gallery-bar>
    </objects>
  </gallery>
</root>
